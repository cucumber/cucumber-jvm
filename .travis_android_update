#!/usr/bin/env expect

set timeout 120 
spawn android update sdk --filter build-tools-21.0.0,platform-tools,extra-android-support,android-21,android-19,sys-img-armeabi-v7a-android-19 --no-ui --force --all
while {1} {
  expect {
    eof               {break}
    "Do you accept"   {send "y\r"}
  }
}
wait
